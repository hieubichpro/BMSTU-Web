{
  "App": {
    "UseDB": "MongoDB"
  },
  "MongoDB": {
    "ConnectionString": "mongodb://localhost:27017/"
  },
  "PostgreSQL": {
    "ConnectionString": "Server=localhost;Database=Web2024;Username=postgres;Password=123456789"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "../../../logs/log.txt",
          "OutputTemplate": "{Timestamp:u} [{Level}] {Message}{NewLine}{Exception}"
        }
      }
    ]
  }

}